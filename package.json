{
  "name": "gosh-fetch",
  "version": "1.1.1",
  "description": "Gosh Fetch - a modern download manager with native Rust engine",
  "main": "dist-electron/src-electron/main.js",
  "type": "module",
  "license": "AGPL-3.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "electron:dev": "npm run build:electron && concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && npm run build:electron && electron-builder",
    "build:electron": "tsc -p tsconfig.node.json",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@reduxjs/toolkit": "^2.5.0",
    "electron-updater": "^6.7.3",
    "lucide-react": "^0.563.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.1.0",
    "electron": "^33.3.0",
    "electron-builder": "^25.1.0",
    "jsdom": "^28.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.0.0",
    "vitest": "^4.0.18",
    "wait-on": "^8.0.0"
  }
}
